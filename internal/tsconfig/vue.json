{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@x/tsconfig-vue",
  "extends": "./web.json",
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "noEmit": true,
    "moduleResolution": "Bundler",
    "module": "ESNext",
    "jsxFactory": "h",
    "jsxImportSource": "vue",
    "declaration": false,
    "declarationMap": false
  },
  "vueCompilerOptions": {
    "target": 3
  }
}
